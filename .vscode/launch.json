    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "pandora login",
        "runtimeArgs": ["-r", "ts-node/register"],
        "args": [
          "src/index.ts",
          "pandora",
          "login"
        ],
        "cwd": "${workspaceRoot}/packages/cli",
        "restart": false,
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen"
      },
      {
        "type": "node",
        "request": "launch",
        "name": "pandora feedback",
        "runtimeArgs": ["-r", "ts-node/register"],
        "args": [
          "src/index.ts",
          "pandora",
          "feedback"
        ],
        "cwd": "${workspaceRoot}/packages/cli",
        "restart": false,
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen"
      }
    ]
}